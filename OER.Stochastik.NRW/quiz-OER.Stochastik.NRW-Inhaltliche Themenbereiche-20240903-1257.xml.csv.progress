(function() {
  ""use strict"";
  var menuGroup = null;
  var id = document.currentScript.closest("".que.stack"").id;
  var key = id + ""-menu-state"";

  function update(state) {
    if (!menuGroup) {
      console.warn(""QMenu.update: No menu-group found"");
      return;
    }

    menuGroup.querySelectorAll(""li"").forEach(function (item, ind) {
      if (ind == state) {
        item.classList.add(""active"");
      } else {
        item.classList.remove(""active"");
      }
    });
  }

  function load() {
    const state = window.sessionStorage.getItem(key);
    if (state) {
      return state;
    } else {
      return 0;
    }
  }

  function storeAndUpdate(state) {
    window.sessionStorage.setItem(key, state);
    update(state);
  }

  document.addEventListener(""DOMContentLoaded"", function(event) {
    menuGroup = document.getElementById(id).querySelector("".menu-group"");
    update(load());
  });

  var QMenu = {};
  QMenu.update = update;
  QMenu.storeAndUpdate = storeAndUpdate;
  window.QMenu = QMenu;
}),
