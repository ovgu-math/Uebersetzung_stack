    update(state);
  }

  document.addEventListener(""DOMContentLoaded"", function(event) {
    menuGroup = document.getElementById(id).querySelector("".menu-group"");
    update(load());
  });

  var QMenu = {};
  QMenu.update = update;
  QMenu.storeAndUpdate = storeAndUpdate;
  window.QMenu = QMenu;
})();
