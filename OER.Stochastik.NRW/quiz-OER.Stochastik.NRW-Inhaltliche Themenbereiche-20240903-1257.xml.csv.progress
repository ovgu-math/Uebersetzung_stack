var error = (n, k, p) => JXG.Math.binomial(n, k) * Math.pow(p, k) * Math.pow(1 - p, n - k) - Math.exp(-n * p) * Math.pow(n * p, k) / JXG.Math.factorial(k);
board.create('functiongraph', [p => error(n.Value(), k.Value(), p), 0, 1], { highlight: false });
board.update();
[[/jsxgraph]]
